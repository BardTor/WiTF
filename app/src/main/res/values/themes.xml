<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.WiTF" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">#578429</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:fontFamily">@font/lato</item>
        <!-- Customize your theme here. -->


    </style>

    <style name="calendar_theme" parent="ThemeOverlay.AppCompat.Dialog">
        <item name="colorSecondary">#99c36f</item>
        <item name="android:windowBackground">@color/vegan_secondary</item>
        <item name="android:colorControlActivated">#99c36f</item>
        <item name="android:textColorPrimary">@color/cardview_dark_background</item>
        <item name="android:buttonBarPositiveButtonStyle">@color/white</item>
        <item name="android:buttonBarNegativeButtonStyle">@color/white</item>
    </style>

</resources>